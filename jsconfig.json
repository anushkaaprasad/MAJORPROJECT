{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2015",
    "lib": ["dom", "dom.iterable", "esnext"]
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
} 